{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "applicationForm.json",
  "definitions": {
    "relationships": {
      "type": "object",
      "properties": {
        "application": {
          "type": "object",
          "properties": {
            "data": {
              "type": "object",
              "properties": {
                "id": { "$ref": "types.json#/definitions/identifier" },
                "type": { "type": "string", "enum": [ "application" ] }
              }
            }
          },
          "additionalProperties": false
        }
      },
      "required": [

      ],
      "additionalProperties": false
    },
    "applicationForm": {
      "title": "Application Form",
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "const": "applicationForm"
        },
        "id": {
          "$ref": "types.json#/definitions/identifier"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "email": { "$ref": "types.json#/definitions/email" },
            "url": {
              "oneOf": [
                {"type": "string"}
              ]
            },
            "stage": {
              "type": "string"
            },
            "applicantDetails": {"$ref": "createApplicationForm.json#/definitions/prefilled"},
            "settingsOverride": {"$ref": "createApplicationForm.json#/definitions/settingsOverride"},
            "tags": {
              "$ref": "tags.json#/definitions/tags"
            },
            "allowedApplicationTypes": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [ "Individual", "Business", "SoleProprietorship" ]
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "url"
          ]
        },
        "relationships": {
          "$ref": "#/definitions/relationships"
        },
        "officerIsBeneficialOwner": { "type": "boolean", "default": false }
      },
      "additionalProperties": false,
      "required": [
        "type",
        "id",
        "attributes"
      ]
    }
  },
  "title": "Application Form Document",
  "type": "object",
  "properties": {
    "data": {
      "$ref": "#/definitions/applicationForm"
    },
    "included": {
      "type": "array"
    }
  },
  "additionalProperties": false,
  "required": [
    "data"
  ]
}